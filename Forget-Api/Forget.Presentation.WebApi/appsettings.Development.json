{
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=Your_DB;Trusted_Connection=true;MultipleActiveResultSets=true",
    "IdentityConnection": "Server=localhost;Database=Your_DB;Trusted_Connection=true;MultipleActiveResultSets=true"
  },
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JWTSettings": {
    "Key": "Your_SecretKey",
    "Issuer": "Your_Issuer",
    "Audience": "Your_Audience",
    "DurationInMinutes": 60
  },
  "MailSettings": {
    "EmailFrom": "Your_Email",
    "SmtpHost": "Your_Host",
    "SmtpPort": 587,
    "SmtpUser": "Your_User",
    "SmtpPass": "Your_Password",
    "DisplayName": "Your_DisplayName"
  }
}